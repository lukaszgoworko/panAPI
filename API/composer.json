{
	"name" : "LukaszGoworko/panAPI",
	"description" : "The API for pan App",
	"license" : "MIT",
	"type" : "project",
	"require" : {
		"php" : ">=7.1",
		"silex/silex" : "~2.0",
		"silex/web-profiler" : "~2.0",
		"cnam/security-jwt-service-provider": "~2.1",
		"monolog/monolog": "^1.22"
	},
	"autoload" : {
		"psr-4" : {
			"LukaszGoworko\\pan\\api\\" : "src/",
			"LukaszGoworko\\pan\\api\\App\\" : "app/",
			"LukaszGoworko\\pan\\api\\UserContext\\" : "contexts/UserContext/src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"LukaszGoworko\\pan\\api\\Tests\\" : "tests/",
			"LukaszGoworko\\pan\\api\\UserContext\\Tests\\" : "contexts/UserContext/tests/"
		}
	},
	"scripts" : {
		"unit" : [
			"echo 'start running unit tests'",
			"vendor/bin/phpunit --colors=always"
		]
	},
	"require-dev" : {
		"phpunit/phpunit" : "~6.1",
		"symfony/browser-kit": "^3.2",
		"symfony/css-selector": "^3.2"
	}
}
